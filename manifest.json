{
  "name": "Google Translate Better Tabs",
  "description": "Changes tab titles to match the language pair you're translating",
  "homepage_url": "https://github.com/chris56974/google-translate-better-tabs",
  "version": "1.00",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "imgs/16x16.png",
      "32": "imgs/32x32.png",
      "48": "imgs/48x48.png",
      "128": "imgs/128x128.png"
    }
  },
  "icons": {
    "16": "imgs/16x16.png",
    "32": "imgs/32x32.png",
    "48": "imgs/48x48.png",
    "128": "imgs/128x128.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://translate.google.com/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ]
}